{
	"manifest_version": 3,
	"name": "Extension",
	"version": "0.0.0",
	"description": "Chrome extension",
	"icons": {
		"16": "images/icon-16.png",
		"32": "images/icon-32.png",
		"48": "images/icon-48.png",
		"128": "images/icon-128.png"
	},
	"action": {
		"default_title": "Extension"
	},
	"content_scripts": [
		{
			"js": ["main.js"],
			"matches": ["<all_urls>"]
		}
	],
	"permissions": ["activeTab", "scripting"],
	"background": {
		"service_worker": "background.js"
	}
}



